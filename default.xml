<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- The FusionSP project -->
  <remote name="fusion"
           fetch="https://github.com/FusionSP" />

  <!-- Fusion remote -->
  <remote name="dragon"
           fetch="https://github.com/Dragon7780" />
           
  <!-- SlimRoms remote -->
  <remote name="slimroms"
           fetch="git://github.com/SlimRoms/" />

  <!-- Omni remote -->
  <remote name="omnirom"
           fetch="https://github.com/omnirom"
           review="gerrit.omnirom.org" />
  
  <!-- AOSP remote -->
  <remote name="aosp"
           fetch="https://android.googlesource.com"
           review="android-review.googlesource.com" />

  <!-- Github remote -->
  <remote name="github"
           fetch=".."
           review="review.cyanogenmod.org" />

  <default revision="refs/heads/cm-11.0"
           remote="github"
           sync-c="true"
           sync-j="8" />
           

  <project path="frameworks/native" name="android_frameworks_native" remote="fusion" revision="cm-11.0" />
  <project path="frameworks/base" name="android_frameworks_base" remote="fusion" revision="cm-11.0" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="fusion" revision="cm-11.0" />

</manifest>
